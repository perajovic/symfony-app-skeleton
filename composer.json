{
    "name": "syx/app-skeleton",
    "license": "proprietary",
    "type": "project",
    "description": "Skeleton for creating a brand new Symfony app",
    "authors": [
        {
            "name": "Pera Jovic",
            "email": "perajovic@me.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/symfony": "~3.1",
        "doctrine/orm": "~2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/monolog-bundle": "^2.11",
        "sensio/distribution-bundle": "^5.0",
        "incenteev/composer-parameter-handler": "^2.1",
        "filos/framework-bundle": "^0.1",
        "doctrine/doctrine-migrations-bundle": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.0",
        "phpunit/phpunit": "^5.0",
        "symfony/phpunit-bridge": "^3.1"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters/common.yml",
            "dist-file": "app/config/parameters.yml.dist"
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
